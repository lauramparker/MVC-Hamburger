<h1>Eat-Da-Burger!</h1>

<ol>
   {{#each burgers}}
  	<li>
  	   <p>ID: {{this.id}}</p>
       <p>Wish: {{this.burger}}</p>
  	</li>
  {{/each}}

</ol>

<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="burger">Burger Name:</label>
    <input type="text" name="name">
  </div>

  <button type="submit">Add Burger</button>
</form>